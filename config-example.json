{
  "datasources": {
    "dbpedia": {
      "description": "DBpedia 3.9 with a Virtuoso back-end",
      "type": "SparqlDatasource",
      "settings": ["http://dbpedia.restdesc.org/", "http://dbpedia.org"]
    },
    "dbpedialevelgraph": {
      "description": "DBpedia 3.9 with a LevelGraph back-end",
      "type": "LevelGraphDatasource",
      "settings": ["data/dbpedia"]
    },
    "ckan": {
      "description": "CKAN data from a SPARQL endpoint",
      "type": "SparqlDatasource",
      "settings": ["http://semantic.ckan.net/sparql","http://datahub.io"]
    },
    "eventmedia": {
      "description": "EventMedia from EURECOM",
      "type": "SparqlDatasource",
      "settings": ["http://eventmedia.eurecom.fr/sparql", ""]
    },
    "stations": {
      "type": "N3Datasource",
      "description": "Belgian railway stations from a Turtle document",
      "settings": ["http://demo.thedatatank.org/railways/stations.ttl"]
    },
    "stationscsv": {
      "description": "Belgian railway stations from a CSV document",
      "type": "CSVDatasource",
      "settings": ["http://data.irail.be/NMBS/Stations.csv", { "delimiter": ";" }]
    },
    "coffee": {
      "description": "MMLab Coffee Data from a Google Docs document",
      "type": "CSVDatasource",
      "settings": ["https://docs.google.com/spreadsheet/pub?key=0Au5ixQ50Pvv_dG5FS294M0RnNDJTOEtDckd4VFB4RVE&single=true&gid=0&output=csv", { "delimiter": "," }]
    }
  },

  "prefixes": {
    "rdf":         "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs":        "http://www.w3.org/2000/01/rdf-schema#",
    "owl":         "http://www.w3.org/2002/07/owl#",
    "skos":        "http://www.w3.org/2004/02/skos/core#",
    "xsd":         "http://www.w3.org/2001/XMLSchema#",
    "dc":          "http://purl.org/dc/terms/",
    "dcterms":     "http://purl.org/dc/terms/",
    "dc11":        "http://purl.org/dc/elements/1.1/",
    "foaf":        "http://xmlns.com/foaf/0.1/",
    "geo":         "http://www.w3.org/2003/01/geo/wgs84_pos#",
    "dbpedia":     "http://dbpedia.org/resource/",
    "dbpedia-owl": "http://dbpedia.org/ontology/",
    "dbpprop":     "http://dbpedia.org/property/",
    "hydra":       "http://www.w3.org/ns/hydra/core#",
    "void":        "http://rdfs.org/ns/void#"
  }
}
